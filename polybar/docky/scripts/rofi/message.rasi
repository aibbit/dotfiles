/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @bg;
    text-color:             @fg;
	font:				 	"Monaco 12";
}

window {
    width:      			320px;
    padding:    			25px;
    border:					2px 2px 2px 2px;
    border-radius:          0px;
    border-color:           @ac;
    location:               center;
    y-offset:               0;
}

entry {
    expand: 				true;
    text-color:				@ac;
}
